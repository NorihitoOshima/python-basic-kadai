        "def calculate_total(price,tax):\n",
        "  #与えられた引数priceに消費税を乗算し、変数totalに代入する\n",
        "  total = price + (price * tax)/100\n",
        "  return total\n",
        "\n",
        "price = 500  # 商品の金額\n",
        "tax = 10  # 消費税率（10%）\n",
        "\n",
        "# calculate_total関数を呼び出してtotalを計算\n",
        "\n",
        "calculate_total\n",
        "\n",
        "# 計算結果を出力\n",
        "print(total)\n",
        "\n",
        "\n"
